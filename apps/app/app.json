{
  "expo": {
    "name": "모동숲 거래장터",
    "slug": "acnh-trading",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "scheme": "acnh-trading",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFF0"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.actrading.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFF0"
      },
      "package": "com.actrading.app",
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-secure-store",
      "@react-native-google-signin/google-signin"
    ],
    "extra": {
      "eas": {
        "projectId": "5f202b10-28ae-41b1-80db-f9273916c55d"
      }
    },
    "owner": "mycindy0710"
  }
}
